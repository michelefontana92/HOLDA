<root_node>
    <task>
        <n_features> 103 </n_features>
        <n_classes>2</n_classes>
        <target>income</target>
    </task>
    <model>
        <model_fn>models.nn.create_simple_net</model_fn>
        <params>
            <input>103</input>
            <hidden_1>200</hidden_1>
            <dropout>0.2</dropout>
            <output>2</output>
        </params>
    </model>
    <metrics>
        <metric>accuracy</metric>
        <metric>precision</metric>
        <metric>recall</metric>
        <metric>f1</metric>
    </metrics>
    <setting>
        <use_deltas>True</use_deltas>
    </setting>

    <architecture>
        <server>
            <training>
                <epochs>2</epochs>
                <patience>10</patience>
                <epoch2ckpt>10</epoch2ckpt>
                <sample_size>1</sample_size>

            </training>
            <proxy>
                <training>
                    <epochs>2</epochs>
                    <patience>10</patience>
                    <epoch2ckpt>10</epoch2ckpt>
                    <sample_size>1</sample_size>

                </training>

                <pers_training>
                    <epochs>3</epochs>
                    <patience>10</patience>
                    <epoch2ckpt>10</epoch2ckpt>
                    <sample_size>1</sample_size>
                </pers_training>
                <client>

                    <training>
                        <epochs>2</epochs>
                        <batch_size>128</batch_size>
                        <patience>10</patience>
                        <epoch2ckpt>10</epoch2ckpt>
                        <use_weights>True</use_weights>
                        <optimizer>
                            <optimizer_fn>torch.optim.Adam</optimizer_fn>
                            <params>
                                <lr>1e-4</lr>
                            </params>
                        </optimizer>
                        <loss>
                            <loss_fn>torch.nn.CrossEntropyLoss</loss_fn>
                        </loss>
                    </training>
                    <pers_training>
                        <epochs>5</epochs>
                        <optimizer>
                            <optimizer_fn>torch.optim.Adam</optimizer_fn>
                            <params>
                                <lr>1e-4</lr>
                            </params>
                        </optimizer>
                    </pers_training>
                </client>
                <client>

                    <training>
                        <epochs>2</epochs>
                        <batch_size>128</batch_size>
                        <patience>10</patience>
                        <epoch2ckpt>10</epoch2ckpt>
                        <use_weights>True</use_weights>
                        <optimizer>
                            <optimizer_fn>torch.optim.Adam</optimizer_fn>
                            <params>
                                <lr>1e-4</lr>
                            </params>
                        </optimizer>
                        <loss>
                            <loss_fn>torch.nn.CrossEntropyLoss</loss_fn>
                        </loss>
                    </training>
                    <pers_training>
                        <epochs>5</epochs>
                        <optimizer>
                            <optimizer_fn>torch.optim.Adam</optimizer_fn>
                            <params>
                                <lr>1e-4</lr>
                            </params>
                        </optimizer>
                    </pers_training>
                </client>

            </proxy>
            <proxy>
                <training>
                    <epochs>2</epochs>
                    <patience>10</patience>
                    <epoch2ckpt>10</epoch2ckpt>
                    <sample_size>1</sample_size>

                </training>

                <pers_training>
                    <epochs>3</epochs>
                    <patience>10</patience>
                    <epoch2ckpt>10</epoch2ckpt>
                    <sample_size>1</sample_size>
                </pers_training>
                <client>

                    <training>
                        <epochs>2</epochs>
                        <batch_size>128</batch_size>
                        <patience>10</patience>
                        <epoch2ckpt>10</epoch2ckpt>
                        <use_weights>True</use_weights>
                        <optimizer>
                            <optimizer_fn>torch.optim.Adam</optimizer_fn>
                            <params>
                                <lr>1e-4</lr>
